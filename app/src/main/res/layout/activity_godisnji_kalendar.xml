<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="@color/colorBraon"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.interfacemockup.kalendar.GodisnjiKalendar">


    <ListView
        android:id="@+id/idDvanaestMeseciListView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:dividerHeight="4dp"
        android:scrollbarThumbVertical="@color/colorOrange"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_godina_textView" />

    <TextView
        android:id="@+id/id_godina_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/_2020_godina"
        android:textAlignment="center"
        android:textColor="@color/colorSiviText"
        android:textSize="22sp"
        android:fontFamily="@font_d/mirosln"
        app:layout_constraintBottom_toTopOf="@+id/idDvanaestMeseciListView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
